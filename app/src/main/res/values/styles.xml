    </style>

    <style name="AppTheme5" parent="Theme.AppCompat.Light.NoActionBar">

    </style>

    <style name="AppTheme6" parent="Theme.AppCompat.Light.NoActionBar">

    </style>

    <style name="AppTheme7" parent="Theme.AppCompat.Light.NoActionBar">

    </style>

    <style name="AppTheme8" parent="Theme.AppCompat.Light.NoActionBar">

    </style>

    <style name="AppTheme9" parent="Theme.AppCompat.Light.NoActionBar">

    </style>

    <style name="AppTheme10" parent="Theme.AppCompat.Light.NoActionBar">

    </style>

    <style name="DrawerArrowStyle" parent="Widget.AppCompat.DrawerArrowToggle">
        <item name="spinBars">true</item>
        <item name="color">@android:color/white</item>
    </style>

    <style name="OverflowStyle" parent="Widget.AppCompat.ActionButton.Overflow">
        <item name="android:src">@drawable/ic_action_navigation_more_vert</item>
    </style>
</resources>
